<main class="form-wrapper">
  <h1 class="form-title">Welcome, <%= accountData.account_firstname %>!</h1>

  <p class="form-subtitle">This is your account dashboard.</p>

  <ul class="form-list">
    <li>Name: <%= accountData.account_firstname %> <%= accountData.account_lastname %></li>
    <li>Email: <%= accountData.account_email %></li>
    <li>Type: <%= accountData.account_type %></li>
  </ul>

  <a href="/account/logout" class="button">Log out</a>
</main>
